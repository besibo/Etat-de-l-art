	Journal = {Mol Ecol},
	Journal-Full = {Molecular ecology},
	Mesh = {Animals; Betula; Genetics, Population; Genotype; Humans; Microsatellite Repeats; Polymorphism, Genetic; Rana temporaria; Research Design; Selection Bias; Statistics as Topic; Ursidae},
	Month = {Nov},
	Number = {11},
	Pages = {3261-73},
